#include <stdio.h>
#include <string.h>
int main()
{
	int a = 10;
	int arr[] = { 1,2,3,4,5,6 };//6*4=24 ->int[6]
	printf("%d\n", sizeof(a));//4
	printf("%d\n", sizeof(int));//4
	printf("%d\n", sizeof a);//4
	printf("%d\n", sizeof(arr));//24
	printf("%d\n", sizeof(arr)/sizeof(arr[0]));//6
	return 0;
}
