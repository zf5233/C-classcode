#include<stdio.h>
//统计num的二进制（补码）中有几个1
int main()
{
	int num = 0;
	int count = 0;
	scanf("%d", &num);
	//32个bit位
	//先和1按位与，再向右移一位，再和1按位与
	int i = 0;
	for (i = 0; i < 32; i++)
	{
		if (1 == ((num >> i) & 1))
			count++;
	}
	printf("%d\n", count);

	/*while (num)
	{
		if (num % 2 == 1)
			count++;
		num = num / 2;
	}*/
	//printf("%d\n", count);
	return 0;
}
